{
  "name": "coc-docker",
  "version": "0.1.2",
  "description": "docker extension for coc",
  "author": "josa@gesell.me",
  "license": "MIT",
  "main": "lib/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/josa42/coc-docker.git"
  },
  "engines": {
    "coc": "^0.0.65"
  },
  "keywords": [
    "coc.nvim",
    "languageserver",
    "docker"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc --watch -p tsconfig.json",
    "prepare": "yarn clean && yarn build"
  },
  "activationEvents": [
    "onLanguage:Dockerfile",
    "onCommand:docker.version"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "docker",
      "properties": {
        "docker.enable": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "commands": [
      {
        "command": "docker.version",
        "title": "Print extension version"
      }
    ]
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.17",
    "@types/node": "^10.14.5",
    "coc.nvim": "0.0.67",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "dockerfile-language-server-nodejs": "^0.0.20",
    "tslib": "^1.9.3"
  }
}
